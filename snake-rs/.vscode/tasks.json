{
    "version": "2.0.0",
       "tasks": [
        {
            "label": "Build Server (Release)",
            "type": "shell",
            "command": "cargo build --release",
            "options": {
                "cwd": "${workspaceFolder}/"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "Run Server",
            "type": "shell",
            "command": " ${workspaceFolder}/target/release/snake-rs.exe",
            "args": [ ],
            "options": {
                "cwd": "${workspaceFolder}/"
            },
            "group": {
                "kind": "none",
                "isDefault": false
            },
            "problemMatcher": []
        }
    ]
}